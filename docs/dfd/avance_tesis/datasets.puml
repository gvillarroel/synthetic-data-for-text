
@startuml
!include ../tema/uchile.puml
left to right direction

skinparam Padding 4
skinparam Margin 0
'skinparam PageMargin 0
scale 5000 width

rectangle "Metricas" as mss {
    card ms [
        = Metricas
        <$WHITE>|<$GREEN><color:$WHITE> SDMetric Score |
        <$WHITE>|<$GREEN><color:$WHITE> Distancia\n<color:$WHITE> Minima a\n<color:$WHITE> otro registro\n<color:$WHITE>(DCR) |
    ]
}

rectangle "Datasets" as ds {
    card dss [
        = Tabular
        <$WHITE>|<$GREEN><color:$WHITE> KingCounty |
        = Tabular
        = & Texto
        <$WHITE>|<$GREEN><color:$WHITE> Economicos |
    ]
}

@enduml